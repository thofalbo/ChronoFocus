@model Permissao

<form>
    <div class="form-group">
        <label asp-for="Controlador"></label>
        <input asp-for="Controlador" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Rota"></label>
        <input asp-for="Rota" class="form-control" />
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary" onclick="permissaoUsuario.fnSubmit('mostrarPermissao', 'cadastrar')">Search</button>
    </div>
</form>

<div id="mostrarPermissao"></div>

<script src="~/js/Pages/permissaoUsuario.js" asp-append-version="true"></script>
<script>
    permissaoUsuario.init({
        urls: {
            index: '@Url.Action("inicio", "permissao-usuario")',
            cadastrar: '@Url.Action("cadastrar", "permissao-usuario")',
            submit: '@Url.Action("cadastrar", "permissao-usuario")'
        }
    });
</script>